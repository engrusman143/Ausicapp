<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.ausicapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".songsActivity"
            android:label="@string/songs_label"/>
        <activity android:name=".playlistActivity"
            android:label="@string/playlist_label"/>
        <activity android:name=".artistActivity"
            android:label="@string/artist_label"/>
        <activity android:name=".scheduleActivity"
            android:label="@string/schedule_label"/>
        <activity android:name=".shareActivity"
            android:label="@string/share_label"></activity>
    </application>

</manifest>